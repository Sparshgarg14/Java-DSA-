public class CodeForces {
    public static void main(String[] args) throws IOException {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        FastReader fr = new FastReader();
        int nmax = (int) (1e5+7);
        int t = fr.nextInt();
        while (t-- > 0) {
         int n = fr.nextInt();
         long x  =fr.nextLong();
         long a[] = new long[n];
            for (int i = 0; i < n; i++) {
                a[i] = fr.nextLong();
            }
            long lo = 0;
            long hi = 2000000007;

            while(lo<hi){
                long mid = lo + (hi-lo+1)/2;
                long water=0;
                for (int i = 0; i < n; i++) {
                     water  += Math.max(mid - a[i], 0L);
                }
                if(water<= x)lo = mid;
                else hi = mid-1;
            }
            System.out.println(lo);



        }
    }







    public static class Pair {
        public long a;
        public long  b;
        public int hashCode;

        Pair(long a, long b) {
            this.a = a;
            this.b = b;
            this.hashCode = Objects.hash(a, b);
        }


        @Override
        public String toString() {
            return a + " -> " + b;
        }


        @Override
        public boolean equals(Object o) {
            if (this == o)
                return true;
            if (o == null || getClass() != o.getClass())
                return false;
            Pair that = (Pair) o;
            return a == that.a && b == that.b;
        }

        @Override
        public int hashCode() {
            return this.hashCode;
        }
    }

}




class FastReader {
    BufferedReader br;
    StringTokenizer st;

    public FastReader() {
        br = new BufferedReader(new InputStreamReader(System.in));
    }

    String next() {
        while (st == null || !st.hasMoreElements()) {
            try {
                String line = br.readLine();

                if (line == null) {
                    return null;
                }

                st = new StringTokenizer(line);

            } catch (IOException e) {
                e.printStackTrace();
            }
        }
        return st.nextToken();
    }

    int nextInt() {
        return Integer.parseInt(next());
    }

    long nextLong() {
        return Long.parseLong(next());
    }

    double nextDouble() {
        return Double.parseDouble(next());
    }

    String nextLine() {
        String str = "";
        try {
            if (st.hasMoreTokens()) {
                str = st.nextToken("\n");
            } else {
                str = br.readLine();
            }
        } catch (IOException e) {
            e.printStackTrace();
        }
        return str;
    }
}


