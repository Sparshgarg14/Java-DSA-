class Solution {
    public long maximumMedianSum(int[] nums) {
        Arrays.sort(nums);
        int i =nums.length-1;
        int cnt=nums.length/3;
        long ans=0;
        while(i>=0 && cnt>0){
          if(i==nums.length-1)i--;
          if(i>=0)ans+=(long)(nums[i]);
          cnt--;
          i-=2;
        }
        return ans;
        
    }
}
