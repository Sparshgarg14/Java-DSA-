public class CodeForces {
    public static void main(String[] args) throws IOException {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        FastReader fr = new FastReader();
        int nmax = (int) (1e5 + 7);
        int t = fr.nextInt();
        int MOD = (int) 1e9 + 7;
        while (t-- > 0) {
        String s  = fr.next();
        HashSet<String>result = new HashSet<>();
       findsubsequences(s,0,"",result);
            System.out.println(result.size());

        }
    }
    public static void findsubsequences(String g,int index, String current, HashSet<String>result){
        if(current.length()==3){
            if(isPalindrome(current))result.add(current);
            return ;
        }
        if(index>=g.length())return ;

      findsubsequences(g,index+1, current+g.charAt(index),result);

       findsubsequences(g,index+1,current,result);


    }



    public static boolean isPalindrome(String g){
        int  i =0,j=g.length()-1;
        boolean check = true;
        while(i<j){
            if(g.charAt(i)!= g.charAt(j)){
                check = false;
                break;
            }
            i++;
            j--;
        }
        if(check)return true;
        else return false;
    }






    public static class Pair {
        public int a;
        public int  b;
        public int hashCode;

        Pair(int a, int b) {
            this.a = a;
            this.b = b;
            this.hashCode = Objects.hash(a, b);
        }


        @Override
        public String toString() {
            return a + " -> " + b;
        }


        @Override
        public boolean equals(Object o) {
            if (this == o)
                return true;
            if (o == null || getClass() != o.getClass())
                return false;
            Pair that = (Pair) o;
            return a == that.a && b == that.b;
        }

        @Override
        public int hashCode() {
            return this.hashCode;
        }
    }

}




class FastReader {
    BufferedReader br;
    StringTokenizer st;

    public FastReader() {
        br = new BufferedReader(new InputStreamReader(System.in));
    }

    String next() {
        while (st == null || !st.hasMoreElements()) {
            try {
                String line = br.readLine();

                if (line == null) {
                    return null;
                }

                st = new StringTokenizer(line);

            } catch (IOException e) {
                e.printStackTrace();
            }
        }
        return st.nextToken();
    }

    int nextInt() {
        return Integer.parseInt(next());
    }

    long nextLong() {
        return Long.parseLong(next());
    }

    double nextDouble() {
        return Double.parseDouble(next());
    }

    String nextLine() {
        String str = "";
        try {
            if (st.hasMoreTokens()) {
                str = st.nextToken("\n");
            } else {
                str = br.readLine();
            }
        } catch (IOException e) {
            e.printStackTrace();
        }
        return str;
    }
    public static long gcd(long a, long b) {
        if (b == 0) {
            return a;
        }
        return gcd(b, a % b);
    }

}
